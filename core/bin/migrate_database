#!/usr/bin/env python
# Run any new migrations from the database

import startup  # noqa: autoflake

from core.config import Configuration
from core.model import SessionManager
from core.scripts import AlembicMigrateVersion

session = SessionManager.session(
    Configuration.database_url(), initialize_data=False, initialize_schema=False
)
AlembicMigrateVersion(_db=session).run()
