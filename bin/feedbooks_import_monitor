#!/usr/bin/env python
"""Update the circulation manager server with new books from
Feedbooks collections."""


from palace.api.feedbooks import FeedbooksImportMonitor, FeedbooksOPDSImporter
from palace.core.model import ExternalIntegration
from palace.core.scripts import OPDSImportScript

OPDSImportScript(
    importer_class=FeedbooksOPDSImporter,
    monitor_class=FeedbooksImportMonitor,
    protocol=ExternalIntegration.FEEDBOOKS,
).run()
