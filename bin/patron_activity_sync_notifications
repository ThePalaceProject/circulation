#!/usr/bin/env python
"""Send out notifications to Patrons that need their loan/hold activity synced"""


from palace.manager.scripts.notifications.patron_activity_sync import (
    PatronActivitySyncNotificationScript,
)
from palace.manager.sqlalchemy.session import production_session

PatronActivitySyncNotificationScript(
    production_session(PatronActivitySyncNotificationScript)
).run()
